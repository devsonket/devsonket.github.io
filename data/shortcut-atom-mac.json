                {
                    "definition": "নতুন উইন্ডো খোলা",
                    "code": "⌘ + Shift + N"
                },
                {
                    "definition": "ফাইল খোলা (অনুসন্ধান করতে নাম টাইপ করা)",
                    "code": "⌘ + P"
                },
                {
                    "definition": "নতুন ফাইল খোলা",
                    "code": "⌘ + O"
                },
                {
                    "definition": "ফোল্ডার খোলা",
                    "code": "⌘ + Shift + O"
                },
                {
                    "definition": "সেভ করা",
                    "code": "⌘ + S"
                },
                {
                    "definition": "সেভ এস",
                    "code": "⌘ + Shift + S"
                },
                {
                    "definition": "ট্যাব বন্ধ করা",
                    "code": "⌘ + W"
                },
                {
                    "definition": "উইন্ডো বন্ধ করা",
                    "code": "⌘ + Shift + W"
                },
                {
                    "definition": "এটম পুনরায় লোড করা",
                    "code": "⌘ + Option + R"
                },
                {
                    "definition": "খোলা ফাইলের তালিকা ব্রাউজ করা",
                    "code": "⌘ + B"
